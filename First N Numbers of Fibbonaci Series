import java.util.Scanner;
public class Main{
    
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a=0;
        int b=1;
        int c;
        for(int i=0;i<n;i++){
             c=a;
            System.out.print(a+" ");
             //c=a;
            a=b;
            b=b+c;
        }
    }
}
